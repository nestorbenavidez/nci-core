<p id="notice"><%= notice %></p>

<h1>Calculation Ranges</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th>Insurance program</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calculation_ranges.each do |calculation_range| %>
      <tr>
        <td><%= calculation_range.name %></td>
        <td><%= calculation_range.value %></td>
        <td><%= calculation_range.insurance_program %></td>
        <td><%= link_to 'Show', calculation_range %></td>
        <td><%= link_to 'Edit', edit_calculation_range_path(calculation_range) %></td>
        <td><%= link_to 'Destroy', calculation_range, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calculation Range', new_calculation_range_path %>
